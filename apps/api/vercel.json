{
  "version": 2,
  "name": "powerpay-api",
  "builds": [
    {
      "src": "dist/apps/api/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/apps/api/src/main.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "dist/apps/api/src/main.js": {
      "includeFiles": "dist/**"
    }
  }
}